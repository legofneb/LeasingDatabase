@{
    ViewBag.Title = "Editor";
    Layout = "~/Views/Shared/_LayoutCustom.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <h2>Editor</h2>

        <form role="form" action="~/Forms/EditorChanged" method="post">
            <textarea id="html" name="html" rows="100" class="form-control">
                
            </textarea>
            <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div>
</div>
<script type="text/javascript">
    
    $("#html").val('@Html.Raw(ViewBag.HTML)');

</script>